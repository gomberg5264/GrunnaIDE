{
  "name": "GrunnaIDE",
  "version": "1.0.0",
  "adonis-version": "4.1.0",
  "description": "Grunna IDE is a open source web IDE",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "test": "node ace test",
    "terser": "./node_modules/terser/bin/terser ./resources/script/*.js --source-map \"root='http://grunna.com/script/',url='app.min.js.map'\" --output ./public/script/app.min.js",
    "js:watch": "onchange \"./resources/script/*.js\" -- npm run terser"
  },
  "keywords": [
    "adonisjs",
    "adonis-app",
    "web-ide",
    "online-ide",
    "grunna"
  ],
  "author": "Daniel Ekedahl <npm@grunna.com> (https://grunna.com)",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/ally": "^2.1.3",
    "@adonisjs/auth": "^3.0.7",
    "@adonisjs/bodyparser": "^2.0.9",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.13",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/session": "^1.0.29",
    "@adonisjs/shield": "^1.0.8",
    "@adonisjs/websocket": "^1.0.11",
    "codemirror": "^5.52.0",
    "directory-tree": "^2.2.3",
    "dockerode": "^2.5.8",
    "fs-extra": "^8.1.0",
    "get-folder-size": "^2.0.1",
    "isomorphic-git": "^0.57.1",
    "mysql": "^2.17.1",
    "promise": "^8.1.0",
    "sqlite3": "^4.2.0",
    "strip-ansi": "^5.2.0",
    "uuid": "^3.3.3",
    "xterm": "^4.4.0",
    "xterm-addon-fit": "^0.3.0"
  },
  "devDependencies": {
    "onchange": "^6.0.0",
    "terser": "^4.6.11"
  },
  "autoload": {
    "App": "./app"
  }
}
